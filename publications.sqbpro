"" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="47"/><column index="2" value="171"/><column index="3" value="85"/><column index="4" value="36"/><column index="5" value="62"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="titleauthor" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="93"/><column index="2" value="54"/><column index="3" value="48"/><column index="4" value="65"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="titles" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="54"/><column index="2" value="300"/><column index="3" value="101"/><column index="4" value="47"/><column index="5" value="62"/><column index="6" value="85"/><column index="7" value="47"/><column index="8" value="61"/><column index="9" value="300"/><column index="10" value="156"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="sql soluiton">select p.pub_name as publisher, a.au_id as author_id, a.au_lname as lastname, a.au_fname as firstname, t.title as title
from authors a
join titleauthor ta on ta.au_id = a.au_id
join titles t on t.title_id = ta.title_id
join publishers p on p.pub_id = t.pub_id; 

-- select distinct(pub_name) from publishers;

select a.au_id as author_id, a.au_lname as lastname, a.au_fname as firstname, p.pub_name as publisher, count(t.title) as title_count
from authors a
join titleauthor ta on ta.au_id = a.au_id
join titles t on t.title_id = ta.title_id
join publishers p on p.pub_id = t.pub_id
group by 1
order by lastname desc;

-- Challenge 3
select a.au_id as author_id, a.au_lname || a.au_fname as auther_name, count(t.title) as title_sold
from authors a
join titleauthor ta on ta.au_id = a.au_id
join titles t on t.title_id = ta.title_id
join publishers p on p.pub_id = t.pub_id
group by author_id
order by title_sold desc
limit 3;

-- Challenge 4

select a.au_id as author_id, a.au_lname || a.au_fname as auther_name, count(t.title) as title_sold
from authors a
join titleauthor ta on ta.au_id = a.au_id
join titles t on t.title_id = ta.title_id
join publishers p on p.pub_id = t.pub_id
group by author_id
order by title_sold desc;</sql><current_tab id="0"/></tab_sql></sqlb_project>
